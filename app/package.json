{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync && sudo npm run smui-theme",
    "smui-theme": "smui-theme compile static/smui.css -i src/theme",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "wasm": "wasm-pack build ./yet_another_dungeon_crawler --target web"
  },
  "devDependencies": {
    "@playwright/test": "^1.21.0",
    "@smui/button": "^6.0.0-beta.16",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.6",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "vite": "^2.9.5",
    "vite-plugin-wasm-pack": "^0.1.10"
  },
  "type": "module",
  "dependencies": {
    "@project-serum/anchor": "^0.22.1",
    "@solana/wallet-adapter-base": "^0.9.2",
    "@solana/wallet-adapter-wallets": "^0.14.2",
    "@solana/web3.js": "^1.32.0",
    "@svelte-on-solana/wallet-adapter-anchor": "^1.0.11-alpha.0",
    "@svelte-on-solana/wallet-adapter-core": "^1.0.2-alpha.0",
    "@svelte-on-solana/wallet-adapter-ui": "^1.0.11-alpha.0"
  }
}
